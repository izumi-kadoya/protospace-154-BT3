<div class="card">
  <%= link_to image_tag("logo.png", class: :card__img ), root_path%>
  <div class="card__body">
    <%= link_to prototype.protoname, root_path, class: :card__title%>
    <p class="card__summary">
      <%= prototype.catchcopy %>
    </p>
    <%= link_to "by #{prototype.user.username}", root_path, class: :card__user %>
  </div>
</div>