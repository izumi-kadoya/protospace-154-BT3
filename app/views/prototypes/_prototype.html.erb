<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img), prototype_path(prototype.id) %>
  <div class="card__body">
    <%= link_to prototype.protoname, prototype_path(prototype.id) ,class: :card__title%>
    <p class="card__summary">
      <%= prototype.catchcopy %>
    </p>
    <%= link_to "by #{prototype.user.username}", root_path, class: :card__user %>
  </div>
</div>