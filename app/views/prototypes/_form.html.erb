<%= form_with model: @prototype, local: true do |f|%>
  <div class="field">
    <%= f.label :protoname, "プロトタイプの名称" %><br />
    <%= f.text_field :protoname, id:"prototype__title" %>
  </div>

  <div class="field">
    <%= f.label :catchcopy, "キャッチコピー" %><br />
    <%= f.text_area :catchcopy, class: :form_text, id:"prototype__detail" %>
  </div>

  <div class="field">
    <%= f.label :concept, "コンセプト" %><br />
    <%= f.text_area :concept, class: :form_text, id:"prototype__concept" %>
  </div>

  <div class="field">
    <%= f.label :image_tag, "プロトタイプの画像" %><br />
    <%= f.file_field :image_tag, id:"prototype__image" %>
  </div>
   
  <div class="actions">
    <%= f.submit "保存する", class: :form_btn  %>
  </div>
<% end %>